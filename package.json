{
  "name": "westend-immer",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copy-static": "cpx static/**/ dist/",
    "gzip": "gzip -k9nr dist",
    "build": "rollup -c && npm run copy-static && npm run gzip",
    "serve": "http-server dist -c0 -g",
    "fmt": "prettier --write 'src/**/*.{ts,tsx}' *.{js,json,md} && npm run lintfix",
    "lint": "tslint -t verbose -c tslint.json 'src/**/*.{ts,tsx}'",
    "lintfix": "tslint -t verbose -c tslint.json --fix 'src/**/*.{ts,tsx}'",
    "fmt_test": "test $(prettier -l 'src/**/*.{ts,tsx}' *.{js,json,md} | wc -l) -eq 0",
    "test": "npm run fmt_test && npm run lint && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "author": "Surma <surma@surma.link>",
  "license": "Apache-2.0",
  "dependencies": {
    "cpx": "^1.5.0",
    "idb-keyval": "^3.1.0",
    "immer": "^1.7.2",
    "preact": "^8.3.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-loadz0r": "^0.4.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript2": "^0.17.1",
    "typescript": "^3.1.3",
    "westend-helpers": "git+ssh://git@github.com/PolymerLabs/westend-helpers.git#master"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "husky": "^1.1.2",
    "prettier": "^1.14.3",
    "tslint": "^5.11.0"
  }
}
